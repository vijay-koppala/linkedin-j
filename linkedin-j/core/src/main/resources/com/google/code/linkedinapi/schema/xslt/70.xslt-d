<xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform"
				xmlns:xs="http://www.w3.org/2001/XMLSchema" 
				version="1.0">
  <xsl:template match="@*|node()">
  	<xsl:apply-templates select="@*|node()"/>
  </xsl:template>
  <xsl:template match="xs:element[@type != 'xs:integer' and @type != 'xs:string' and @type != 'xs:boolean' and @type != 'xs:anyURI']" >
  	name =<xsl:value-of select="@name"/> type=<xsl:value-of select="@type"/>
  </xsl:template> 
  
  
   
</xsl:stylesheet>